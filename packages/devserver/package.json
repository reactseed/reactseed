{
  "name": "@reactseed/devserver",
  "version": "0.1.0",
  "description": "Override webpack devServer",
  "main": "index.js",
  "scripts": {
    "commit": "npx git-cz",
    "prettier": "prettier --write 'src/**/*.js'",
    "lint": "eslint --fix 'src/**/*.js'"
  },
  "keywords": [
    "reactseed",
    "devserver"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reactseed/devserver.git"
  },
  "homepage": "https://github.com/reactseed/devserver",
  "bugs": {
    "url": "https://github.com/reactseed/devserver/issues"
  },
  "license": "BSD",
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "pre-commit": "lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose --no-verify"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1"
  }
}
