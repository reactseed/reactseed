{
  "name": "@reactseed/use-redux",
  "version": "1.0.5",
  "description": "Use redux to drive state with a React hooks",
  "main": "lib/index.js",
  "scripts": {
    "dev": "babel -w src --out-dir lib --ignore \"**/__tests__\"  --extensions \".ts\"",
    "dev:type": "tsc -d --emitDeclarationOnly --watch src/*.ts --outDir lib",
    "build": "babel src --out-dir lib --ignore \"**/__tests__\" --extensions \".ts\"",
    "build:type": "tsc -d --emitDeclarationOnly",
    "commit": "npx git-cz",
    "prettier": "prettier --write 'src/*.ts'",
    "lint": "eslint --fix 'src/*.ts'"
  },
  "keywords": [
    "use-redux",
    "reactseed",
    "immer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reactseed/reactseed.git"
  },
  "homepage": "https://github.com/reactseed/reactseed",
  "bugs": {
    "url": "https://github.com/reactseed/reactseed/issues"
  },
  "license": "BSD",
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "pre-commit": "lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose --no-verify"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": "^16.8.1",
    "react-redux": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-typescript": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^14.0.9",
    "@types/react": "^16.9.35",
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "immer": "^6.0.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "typescript": "^4.4.4"
  },
  "gitHead": "4f66f3e7fa1e9cc92e9a6d8ade9416b814f27f23",
  "publishConfig": {
    "access": "public"
  }
}
