{
  "name": "@reactseed/cli",
  "description": "React seed command-line interface for creating a react app",
  "version": "1.1.1",
  "scripts": {
    "commit": "npx git-cz",
    "prettier": "prettier --write 'src/**/*.js'",
    "lint": "eslint --fix 'src/**/*.js'"
  },
  "main": "./src/bin/reactseed.js",
  "bin": {
    "reactseed": "./src/bin/reactseed.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactseed/reactseed.git"
  },
  "homepage": "https://github.com/reactseed/reactseed",
  "bugs": {
    "url": "https://github.com/reactseed/reactseed/issues"
  },
  "license": "BSD",
  "keywords": [
    "reactseed",
    "react",
    "CLI"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "pre-commit": "lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose --no-verify"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "command-exists": "^1.2.8",
    "commander": "^2.19.0",
    "compressing": "^1.5.0",
    "debug": "^4.1.1",
    "fs-extra": "^9.0.0",
    "get-proxy-settings": "^0.1.13",
    "globby": "^11.0.0",
    "istextorbinary": "^3.3.0",
    "lodash.camelcase": "^4.3.0",
    "mkdirp": "^1.0.4",
    "mz-modules": "^2.1.0",
    "node-homedir": "^1.1.1",
    "proxy-agent": "^3.1.1",
    "urllib": "^2.35.0",
    "yargs-parser": "^11.1.1",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "request": "^2.88.2"
  },
  "gitHead": "4f66f3e7fa1e9cc92e9a6d8ade9416b814f27f23"
}
