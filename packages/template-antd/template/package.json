{
  "name": "__name__",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.35.1",
    "@reactseed/use-redux": "^1.0.5",
    "@reactseed/use-request": "^1.0.8",
    "antd": "^4.19.5",
    "dayjs": "^1.11.1",
    "immer": "^9.0.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --only-mapped",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "commit": "git cz",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "eslint": "eslint --fix --no-error-on-unmatched-pattern 'src/**/*.{ts,tsx}'",
    "stylelint": "stylelint 'src/**/*.less' --custom-syntax postcss-less --allow-empty-input"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint",
      "npm run prettier"
    ],
    "*.{less}": [
      "npm run stylelint"
    ]
  },
  "pre-commit": "lint-staged",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose --no-verify"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@craco/craco": "^7.0.0-alpha.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.3.3",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.5",
    "craco-alias": "^3.0.1",
    "craco-less": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.3"
  }
}
